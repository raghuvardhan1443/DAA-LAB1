def find_max_min(arr):
    if not arr:
        return None, None
    max_val = arr[0]
    min_val = arr[0]
    for num in arr:
        if num > max_val:
            max_val = num
        if num < min_val:
            min_val = num
    return max_val, min_val
arr = [3, 1, 4, 1, 5, 9, 2, 6, 5, 3, 5]
max_value, min_value = find_max_min(arr)
print("Maximum Value:", max_value)
print("Minimum Value:", min_value)
