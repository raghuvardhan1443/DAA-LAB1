def countoddeven(n):
    oddcount = 0
    evencount = 0
    for i in range(1, n + 1):
        if i % 2 == 0:
            evencount += 1
        else:
            oddcount +=  1
    return oddcount, evencount
n = 100  
oddcount, evencount = countoddeven(n)
print(f"In the range from 1 to {n}:")
print(f"Odd integers: {oddcount}")
print(f"Even integers: {evencount}")
